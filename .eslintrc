{
    "extends": ["airbnb", "prettier", "plugin:react/recommended"],
    "plugins": ["prettier"],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "prettier/prettier": ["error"],
      "react/jsx-filename-extension": 0,
      "react/require-default-props": 0
